{
    "name": "cdn77/functions",
    "type": "library",
    "license": "proprietary",
    "description": "PHP Functions",
    "keywords": [
        "php",
        "functions",
        "ds",
        "generators",
        "never"
    ],
    "authors": [
        {
            "name": "Simon Podlipsky",
            "email": "simon@podlipsky.net"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-ds": "*"
    },
    "autoload": {
        "files": ["src/functions_include.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Cdn77\\Functions\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "ext-pcov": "*",
        "cdn77/coding-standard": "^5.0",
        "ergebnis/composer-normalize": "^2.23",
        "icanhazstring/composer-unused": "^0.7.6",
        "infection/infection": "^0.23.0",
        "maglnet/composer-require-checker": "^3.3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.92",
        "phpstan/phpstan-phpunit": "^0.12.21",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "roave/infection-static-analysis-plugin": "^1.8",
        "vimeo/psalm": "^4.8"
    },
    "extra": {
        "unused": [
            "ext-ds"
        ]
    }
}
